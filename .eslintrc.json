{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-one-expression-per-line": "off",
    "@typescript-eslint/quotes": "off",
    "linebreak-style": "off",
    "react/jsx-props-no-spreading": "off"
    // 可以根据项目需求自定义 ESLint 规则
  }
}
